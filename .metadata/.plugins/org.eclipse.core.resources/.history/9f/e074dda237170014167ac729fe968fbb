package com;


import java.util.*;

public class SuffixTreeNode {
	HashMap<Character,SuffixTreeNode> children = new HashMap<Character,SuffixTreeNode>();
	char value;
	ArrayList<Integer> indexes = new ArrayList<Integer>();
	
	public void insertString(String s, int index) {
		indexes.add(index);
		value = s.charAt(0);
	}
	
	

}
